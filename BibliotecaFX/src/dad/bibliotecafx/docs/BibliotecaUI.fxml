<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dad.bibliotecafx.controller.BibliotecaPruebaController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
            <Tab fx:id="librosTabPane" text="Libros">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <GridPane fx:id="librosGridPane" BorderPane.alignment="CENTER">
                           <children>
                              <TextField fx:id="libroText" maxWidth="-Infinity" prefHeight="25.0" prefWidth="249.0" promptText="Título" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="CENTER" GridPane.vgrow="NEVER">
                                 <GridPane.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TableView fx:id="librosTable" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn fx:id="tituloTableColumn" prefWidth="75.0" text="Título" />
                                    <TableColumn fx:id="autorTableColumn" prefWidth="75.0" text="Autor" />
                                    <TableColumn fx:id="anioTableColumn" prefWidth="75.0" text="Año de publicacion" />
                                    <TableColumn fx:id="ejemplaresBiblioTableColumn" prefWidth="75.0" text="Ejemplares en biblioteca" />
                                 </columns>
                              </TableView>
                           </children>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints maxHeight="70.0" minHeight="50.0" prefHeight="70.0" vgrow="SOMETIMES" />
                              <RowConstraints prefHeight="263.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                        </GridPane>
                     </center>
                  </BorderPane>
               </content>
            </Tab>
            <Tab fx:id="usuariosTabPane" text="Usuarios">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <GridPane fx:id="usuariosGridPane" BorderPane.alignment="CENTER">
                           <children>
                              <TextField fx:id="usuarioText" maxWidth="-Infinity" prefHeight="25.0" prefWidth="249.0" promptText="Nombre" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="CENTER" GridPane.vgrow="NEVER">
                                 <GridPane.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TableView fx:id="usuariosTable" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn fx:id="nombreTableColum" prefWidth="75.0" text="Nombre" />
                                    <TableColumn fx:id="apellidosTableColumn" prefWidth="75.0" text="Apellidos" />
                                    <TableColumn fx:id="rolTableColumn" prefWidth="75.0" text="Rol" />
                                 </columns>
                              </TableView>
                           </children>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints maxHeight="70.0" minHeight="50.0" prefHeight="70.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                        </GridPane>
                     </center>
                     <bottom>
                        <BorderPane prefHeight="32.0" prefWidth="239.0" BorderPane.alignment="CENTER">
                           <left>
                              <FlowPane alignment="CENTER_LEFT" prefHeight="32.0" prefWidth="216.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button fx:id="carnetButton" mnemonicParsing="false" onAction="#onGenerarCarnetUsu" text="Generar carnet">
                                       <FlowPane.margin>
                                          <Insets bottom="10.0" top="10.0" />
                                       </FlowPane.margin>
                                    </Button>
                                 </children>
                                 <BorderPane.margin>
                                    <Insets left="10.0" />
                                 </BorderPane.margin>
                              </FlowPane>
                           </left>
                           <right>
                              <FlowPane alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="257.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button fx:id="altaUsuButton" defaultButton="true" mnemonicParsing="false" onAction="#onAltaUsuario" text="Dar de alta">
                                       <FlowPane.margin>
                                          <Insets right="10.0" />
                                       </FlowPane.margin>
                                    </Button>
                                    <Button fx:id="modificarUsuButton" mnemonicParsing="false" onAction="#onModificarUsuario" text="Modificar">
                                       <FlowPane.margin>
                                          <Insets right="10.0" />
                                       </FlowPane.margin>
                                    </Button>
                                    <Button fx:id="bajaUsuButton" mnemonicParsing="false" onAction="#onBajaUsuario" text="Dar de baja">
                                       <FlowPane.margin>
                                          <Insets />
                                       </FlowPane.margin>
                                    </Button>
                                 </children>
                                 <BorderPane.margin>
                                    <Insets right="10.0" />
                                 </BorderPane.margin>
                              </FlowPane>
                           </right>
                        </BorderPane>
                     </bottom>
                  </BorderPane>
               </content>
            </Tab>
            <Tab fx:id="prestamosTabPane" text="Préstamos">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <bottom>
                        <FlowPane fx:id="prestamosFlowPane" alignment="CENTER_RIGHT" prefHeight="32.0" prefWidth="239.0" BorderPane.alignment="CENTER">
                           <children>
                              <Button fx:id="nuevoPresButton" defaultButton="true" mnemonicParsing="false" onAction="#onNuevoPrestamo" text="Nuevo préstamo">
                                 <FlowPane.margin>
                                    <Insets right="10.0" />
                                 </FlowPane.margin>
                              </Button>
                              <Button fx:id="eliminarPresButton" mnemonicParsing="false" onAction="#onEliminarPrestamo" text="Eliminar préstamo" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" right="10.0" top="10.0" />
                           </padding>
                        </FlowPane>
                     </bottom>
                     <center>
                        <GridPane fx:id="prestamosGridPane" BorderPane.alignment="CENTER">
                           <children>
                              <FlowPane fx:id="presBuscarFlowPane" alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="prestamoText" maxWidth="-Infinity" prefHeight="25.0" prefWidth="249.0" promptText="Buscar por...">
                                       <FlowPane.margin>
                                          <Insets right="10.0" />
                                       </FlowPane.margin>
                                    </TextField>
                                    <ComboBox fx:id="buscarPresComboBox" prefWidth="150.0" />
                                 </children>
                                 <GridPane.margin>
                                    <Insets bottom="10.0" left="10.0" top="10.0" />
                                 </GridPane.margin>
                              </FlowPane>
                              <TableView fx:id="prestamosTable" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                                 <columns>
                                    <TableColumn fx:id="codigoPresTableColumn" prefWidth="75.0" text="Código" />
                                    <TableColumn fx:id="libroPresTableColumn" prefWidth="75.0" text="Libro" />
                                    <TableColumn fx:id="usuarioPresTableColumn" prefWidth="75.0" text="Usuario" />
                                    <TableColumn fx:id="fechainiPresTableColumn" prefWidth="75.0" text="Fecha inicial" />
                                 </columns>
                              </TableView>
                           </children>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints maxHeight="70.0" minHeight="50.0" prefHeight="70.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                        </GridPane>
                     </center>
                  </BorderPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
